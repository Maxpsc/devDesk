{
    "manifest_version": 2,
    "name": "devDesk",
    "version": "1.0.0",
    "description": "An elegant Tab desk for developer",
    "browser_action": {
        "default_icon":"icon.png"
    },
    "permissions":[
        "storage",
        "geolocation"
        /*{"fileSystem": ["write", "directory"]}*/
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
